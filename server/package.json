    "license": "MIT",
    "publisher": "SimonSchmidt",
    "engines": {
        "node": "*"
    },
    "dependencies": {
        "fast-glob": "^3.3.2",
        "fast-xml-parser": "^4.3.3",
        "ts-fusion-parser": "^0.1.40",
        "ts-fusion-runtime": "^0.0.42",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.11",
